/*CSS进度环  */
.p_po{
    width: 200px;
    margin:  0 auto;
    height: 200px;
    position: relative;
}
.t_box{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    color: #fff;
}
.w_box{
    height: 240px;
    /* background-color: #1cb1ff;   */
    background-color: #009b63;
    padding-top: 20rpx;
}
.wrap_c,.circle,.percent_s{
    position: absolute;
    width: 200px;  
    height: 200px;  
    border-radius: 50%; 
}
.wrap_c{  
    background-color: #248260;  
}  
.circle{  
    box-sizing: border-box;  
    /* border:8px solid #009ef0;   */
    border: 8px solid #248260;
    clip:rect(0,200px,200px,100px);  
}  
.clip-auto{  
    clip:rect(auto, auto, auto, auto);  
}  
.percent_s{  
    box-sizing: border-box;  
    top:-8px;  
    left:-8px;  
}  
.left{  
    transition:transform ease;  
    border:16px solid #fff;  
    clip: rect(0,100px,200px,0);  
}  
.right{  
    border:16px solid #fff;  
    clip: rect(0,200px,200px,100px);  
}  
.wth0{  
    width:0;  
}  
.num{  
    position: absolute;  
    box-sizing: border-box;  
    width: 183px;  
    height: 183px;  
    line-height: 183px;  
    text-align: center;  
    border:  solid 1px #e4fff5;
    font-size: 40px;  
    left: 8px;  
    top: 8px;  
    border-radius: 50%;  
    background-color: #009b63;  
}  

/*canvas保留样式  */
.wrap_can{
    position: relative;
    padding:  20rpx 0 70rpx 0;
    /* background-image:-webkit-linear-gradient(to top,  #0ce2da ,#1cb1ff);
    background-image:linear-gradient(to top, #0ce2da ,#1cb1ff); */
    background: #1cb1ff;  
    /* background: url(https://m.ly.com/healthy/health/assets/images/small/index_bg.jpg) no-repeat;
    background-size:  cover; */
}
.wrap_text{
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column wrap;
    margin-top: 30px;
    position: relative;
}
.wrap_text .total_s, .wrap_text .num_show,.wrap_text .icon_show{
    display: flex;
    justify-content: center;
    margin: 0 auto;
}
.icon_show{
    margin: 0 auto;
    display: block;
    height: 32px;
    width: 26px;
}
.total_s{
    align-items: center;
    font-size: 13px;
    padding-top: 15px;
    align-self: baseline;
}
.total_s cover-view{
    display: inline-block;
    text-align: center;
    font-size: 12px;
    
}
.total_s .t_item{
    margin-left: 8px;
}
.line_bg{
    width: 1px;
    color: #fff;
    font-size: 10px;
    vertical-align: center;
    line-height: 10px;
    height: 10px;
    background: #fff; 
    margin: 0 6px;
    display: inline-block;
    vertical-align: middle;
}
.bind_t1{
    font-size: 20px;
    margin: 10px 0 8px;
}
.bind_t2{
    font-size: 15px;
    
}
.bind_t1 text{
    display: inline-block;
    position: relative;
    padding-bottom: 14px;
}
.bind_t1 text:after{
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #fff;
    color: #fff;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}
.num_show{
    margin:  0 auto;
    padding-top: 10px;
    font-size: 16px;
    line-height: 1.2;
    align-items: baseline;
}
.num_show text{
    font-size: 42px;
}
.num_show .n_font{
    font-size: 14px;
    display: inline;
}
/*推荐  */
.re_tit{
    padding: 30rpx;
    display: flex;
    justify-content: space-between;
    background: #fff;
    position: relative;
}
.re_tit::after{
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e5e5e5;
    color: #e5e5e5;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}
.re_l{
    font-size: 32rpx;
}
.re_l view{
    display: inline-block;
    width: 32rpx;
    height: 32rpx;
    background: url(https://m.ly.com/healthy/health/assets/images/app/small1/all_icon2.png?t=20171109) no-repeat;
    background-size: 175rpx 249rpx;
    background-position: -2rpx -160rpx;
    margin-right: 14rpx;
}
.re_r{
    position: relative;
    font-size: 24rpx;
    color: #999;
}
.re_r:after{
    content: "";
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    border-right: 2rpx solid #ccc;
    border-bottom: 2rpx solid #ccc;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    font-size: 0;
    margin-left: 20rpx;
}
.re_bg{
    display: flex;
    justify-content: flex-start;
    background: #fff;
    font-size: 28rpx;
}
.re_bg text{
     font-size: 24rpx;
     color: #999;
     padding-top: 10rpx;
     display: block;
}
.re_bg .re_01{
    flex: 0 0 36%;
    padding: 30rpx 0 0 30rpx;
    position: relative;
}
.re_01 image{
    position: absolute;
    top: 0;
    right: 0;
    display: block;
    width: 73rpx;
    height: 74rpx;
    
}
.re_bg .re_02{
    flex: 0 0 63%;
    position: relative;
}
.ac_1{
    position: absolute;
    bottom: 0 ;
    right: 0;
    display: inline-block;
    width: 260rpx;
    height: 188rpx;
    background: url(https://m.ly.com/healthy/health/assets/images/small/index_ag01.jpg?t=20171109) no-repeat;
    background-size: 260rpx 188rpx;
}
.re_02 > view:first-child{
    display: flex;
    justify-content: space-around;
    text-align: center;
    align-items: center;
}
.re_02 .re_item01 .items{
    position: relative;
    flex: 2;
    height: 160rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column wrap;
    text-align: center;
}
.re_02 .re_item01 .items:first-child:after{
    content: " ";
    position: absolute;
    right: 0;
    top: 0;
    width: 1px;
    bottom: 0;
    border-right: 1px solid #e5e5e5;
    color: #e5e5e5;
    transform-origin: 100% 0;
    transform: scaleX(0.5);
}
.items text{
    display: block;
}
.re_02 .re_item02{
    padding: 30rpx 0 0 30rpx;
    height: 130rpx;
    position: relative;
}

.re_bg .re_02:before{
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    width: 1px;
    bottom: 0;
    border-left: 1px solid #e5e5e5;
    color: #e5e5e5;
    transform-origin: 0 0;
    transform: scaleX(0.5);
}
.re_02 .re_item02:before{
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}
.ac_2{
    position: absolute;
    bottom: 14rpx;
    right:14rpx;
    display: inline-block;
    width: 188rpx;
    height: 137rpx;
    background: url(https://m.ly.com/healthy/health/assets/images/small/index_ag02.jpg?t=20171109) no-repeat;
    background-size: 188rpx 137rpx;
}
.cshi{
    padding: 30rpx;
    font-size: 32rpx;
}

.sync_bg{
    font-size: 26rpx;
    color: #f65e51;
    text-align: center;
    padding: 20rpx 30rpx;
    line-height: 46rpx;
}